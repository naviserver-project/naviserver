# -*- Tcl -*-

package require tcltest 2.2
namespace import -force ::tcltest::*

::tcltest::configure {*}$argv

#
# ns_urlspace
#
test ns_urlspace-1.0 {basic operation} -body {
    ns_urlspace
} -returnCodes error -result {wrong # args: should be "ns_urlspace get|list|new|set|unset ?/arg .../"}

test ns_urlspace-1.1 {basic operation} -body {
    ns_urlspace x
} -returnCodes error -result {ns_urlspace: bad subcommand "x": must be get, list, new, set, or unset}

test ns_urlspace-1.2 {syntax ns_urlspace get} -body {
    ns_urlspace get
} -returnCodes error -result {wrong # args: should be "ns_urlspace get ?-context /setId/? ?-exact? ?-id /integer[-1,16]/? ?-key /string/? ?-noinherit? /URL/"}
test ns_urlspace-1.3 {syntax ns_urlspace set} -body {
    ns_urlspace set
} -returnCodes error -result {wrong # args: should be "ns_urlspace set ?-contextfilter /string/? ?-id /integer[-1,16]/? ?-key /string/? ?-noinherit? /URL/ /data/"}
test ns_urlspace-1.4 {syntax ns_urlspace xunset} -body {
    ns_urlspace unset
} -returnCodes error -result {wrong # args: should be "ns_urlspace unset ?-allfilters? ?-id /integer[-1,16]/? ?-key /string/? ?-noinherit? ?-recurse? /URL/"}

test ns_urlspace-1.5 {syntax ns_urlspace new} -body {
    ns_urlspace new ?
} -returnCodes error -result {wrong # args: should be "ns_urlspace new"}

test ns_urlspace-1.6 {syntax ns_urlspace list} -body {
    ns_urlspace list ?
} -returnCodes error -result {wrong # args: should be "ns_urlspace list ?-id /integer[-1,16]/?"}


test ns_urlspace-2.0.0 {ns_urlspace get, when nothing is set} -body {
    ns_urlspace get /
} -result {}


test ns_urlspace-2.1.0 {flat set/get/unset/list} -setup {
    ns_urlspace set / foo
} -body {
    list \
        [ns_urlspace get /] \
        [ns_urlspace unset /] \
        [ns_urlspace unset /] \
        [ns_urlspace get /] \
        [ns_urlspace list]
} -result {foo 1 0 {} {}}

test ns_urlspace-2.1.1 {flat set/get/unset/list, map with query variables} -setup {
    ns_urlspace set / foo
    ns_urlspace set /?x=1 bar
    ns_urlspace set /?x=2 baz
} -body {
    list  \
        [ns_urlspace get /] \
        [ns_urlspace get /?x=1] \
        [ns_urlspace get /?x=2] \
        [ns_urlspace unset /] \
        [ns_urlspace unset /?x=1] \
        [ns_urlspace unset /?x=2] \
        [ns_urlspace get /] \
        [ns_urlspace get /?x=1] \
        [ns_urlspace get /?x=2] \
        [ns_urlspace list]
} -result {foo bar baz 1 1 1 {} {} {} {}}

test ns_urlspace-2.1.2 {flat set/get/unset/list, map with query variables} -setup {
    ns_urlspace set /foo foo
    ns_urlspace set /foo/test.cgi foo.cgi
    ns_urlspace set /bar/*.cgi bar.cgi
} -body {
    list  \
        [ns_urlspace get /foo] \
        [ns_urlspace get /foo/test.cgi] \
        [ns_urlspace get /foo/test.cgi/baz] \
        [ns_urlspace get /bar/test.cgi] \
        [ns_urlspace get /bar/test.cgi/baz] \
        [ns_urlspace unset /foo] \
        [ns_urlspace unset /foo/test.cgi] \
        [ns_urlspace unset /bar/*.cgi] \
        [ns_urlspace list]
} -result {foo foo.cgi foo.cgi bar.cgi {} 1 1 1 {}}

test ns_urlspace-2.2.0 {flat noinherit set/list/unset/list} -setup {
    #ns_log notice "BEFORE <[ns_urlspace list]>"
    ns_urlspace set /x foo
    ns_urlspace set -noinherit /y bar
} -body {
    ns_urlspace list
} -cleanup {
    ns_urlspace unset /x
    ns_urlspace unset -noinherit /y
} -result {{. /x * inherit foo} {. /y * noinherit bar}}


test ns_urlspace-2.2.1 {flat noinherit set/list/unset/list} -setup {
    ns_urlspace set / foo
    ns_urlspace set /x foo
    ns_urlspace set -noinherit /y bar
} -body {
    ns_urlspace list
} -cleanup {
    ns_urlspace unset /
    ns_urlspace unset /x
    ns_urlspace unset -noinherit /y
} -result {{. /x * inherit foo} {. /y * noinherit bar} {. / * inherit foo}}


test ns_urlspace-2.2.2 {flat noinherit set/list/unset/list} -setup {
    #ns_log notice "BEFORE <[ns_urlspace list]>"
    ns_urlspace set /x foo
    ns_urlspace set -noinherit /y bar
} -body {
    list \
        [ns_urlspace list] \
        [ns_urlspace unset /x] \
        [ns_urlspace unset /y] \
        [ns_urlspace list] \
        [ns_urlspace unset -noinherit /y] \
        [ns_urlspace list]
} -result {{{. /x * inherit foo} {. /y * noinherit bar}} 1 0 {{. /y * noinherit bar}} 1 {}}



